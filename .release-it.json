{
  "hooks": {
    "after:bump": "cd ./dist/ && cat ../app.json | jq '.expo.version |= \"${version}\"'"
  },
  "git": {
    "commitMessage": "chore: release ${version}",
    "tagName": "${version}"
  },
  "github": {
    "release": true,
    "draft": true,
    "web": true,
    "autogenerate": true
  }
}
